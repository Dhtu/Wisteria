bpftool map create /sys/fs/bpf/mnt_ns_set type hash key 8 value 4 entries 128 \
        name mnt_ns_set flags 0


 ./tcpconnect --mntnsmap /sys/fs/bpf/mnt_ns_set &
./write --mntnsmap /sys/fs/bpf/mnt_ns_set &


